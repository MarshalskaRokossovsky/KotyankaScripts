              "        result = choice3(",
              "            0.65, -1,",
              "            0.25, 0,",
              "            0.10, 1",
              "        );",
              "    } else if (lvl == 3) {",
              "        result = choice3(",
              "            0.40, -1,",
              "            0.40, 0,",
              "            0.20, 1",
