          [
              "function DragonHungerActCode() { //t_OnStep|0,11",
              "    int hunger;",
              "",
              "    hunger = GetData(0,ScriptItemActShip());",
              "",
              "    hunger = hunger - 1;",
              "",
              "    if(GoodsCount(ScriptItemActShip(),t_Food) > 10) {",
              "        while(GoodsCount(ScriptItemActShip(),t_Food) > 10) {",
              "            GoodsAdd(ScriptItemActShip(),t_Food,-10);",
              "            hunger = hunger + 1;",
              "",
              "            if(hunger == 100) break;",
              "        }",
              "    }",
              "",
              "    if(GetData(0,ScriptItemActShip()) < 70 && hunger >= 70) {",
              "        HullHP(ScriptItemActShip(),'Plus10');",
              "    }",
              "    else if(GetData(0,ScriptItemActShip()) < 50 && hunger >= 50) {",
              "        HullHP(ScriptItemActShip(),'Plus20');",
              "    }",
              "    else if(GetData(0,ScriptItemActShip()) < 40 && hunger >= 40) {",
              "        HullHP(ScriptItemActShip(),'Plus35');",
              "    }",
              "    else if(GetData(0,ScriptItemActShip()) < 30 && hunger >= 30) {",
              "        HullHP(ScriptItemActShip(),'Plus50');",
              "    }",
              "    else if(GetData(0,ScriptItemActShip()) < 20 && hunger >= 20) {",
              "        HullHP(ScriptItemActShip(),'Plus70');",
              "    }",
              "    else if(GetData(0,ScriptItemActShip()) < 10 && hunger >= 10) {",
              "        HullHP(ScriptItemActShip(),'Plus90');",
              "    }",
              "    else HullHP(ScriptItemActShip(),'Plus100');",
              "",
              "",
              "",
              "    if(GetData(0,ScriptItemActShip()) >= 70 && hunger < 70) {",
              "        HullHP(ScriptItemActShip(),'Minus10');",
              "    }",
              "    else if(GetData(0,ScriptItemActShip()) >= 50 && hunger < 50) {",
              "        HullHP(ScriptItemActShip(),'Minus10');",
              "    }",
              "    else if(GetData(0,ScriptItemActShip()) >= 40 && hunger < 40) {",
              "        HullHP(ScriptItemActShip(),'Minus15');",
              "    }",
              "    else if(GetData(0,ScriptItemActShip()) >= 30 && hunger < 30) {",
              "        HullHP(ScriptItemActShip(),'Minus20');",
              "    }",
              "    else if(GetData(0,ScriptItemActShip()) >= 20 && hunger < 20) {",
              "        HullHP(ScriptItemActShip(),'Minus20');",
              "    }",
              "    else if(GetData(0,ScriptItemActShip()) >= 10 && hunger < 10) {",
              "        HullHP(ScriptItemActShip(),'Minus20');",
              "    }",
              "    else CustomLose(\"Вы не кормили дракона, и он помер прямо в космосе, и вы вместе с ним\");",
              "}"          
