                },
                {
                    "Type": "Top",
                    "Name": "",
                    "Pos.x": 250,
                    "Pos.y": -60,
                    "Parent": -1,
                    "#": 22,
                    "Code.Type": "Turn",
                    "Code": 
                    [
                        "for(i = 0;ConStars(temp_sec) >= i;i = i + 1) {",
                        "    if(!ConStar(temp_sec,i)) continue;",
                        "    if(ShipStar(Player()) == ConStar(temp_sec,i)) InjectAnswer('Systems',Name(ConStar(temp_sec,i))+\" (Вы здесь)\",ConStar(temp_sec,i));",
                        "    else continue;",
                        "}",
                        "for(i = 0;ConStars(temp_sec) >= i;i = i + 1) {",
                        "    if(!ConStar(temp_sec,i)) continue;",
                        "    if(ShipStar(Player()) == ConStar(temp_sec,i)) continue;",
                        "    else InjectAnswer('Systems',Name(ConStar(temp_sec,i)),ConStar(temp_sec,i));",
                        "}"
                    ]
                },
                {
                    "Type": "Top",
                    "Name": "",
                    "Pos.x": 330,
                    "Pos.y": -110,
                    "Parent": -1,
                    "#": 25,
                    "Code.Type": "Turn",
                    "Code": 
                    [
                        "j=0;",
                        "for(k=0;StarRuins(temp_star)>k;k=k+1) {",
                        "    if(ShipType(StarRuins(temp_star,k)) == 'WB') j=j+1;",
                        "}",
                        "",
                        "if(StarRuins(temp_star) - j >= 3) tstr2 = \"В системе слишком много станций! Постройка невозможна!\";",
                        "else if(StarOwner(temp_star) != 0 && StarOwner(temp_star) != 2) tstr2 = \"Система захвачена, постройка невозможна!\";",
                        "else if(StarOwner(temp_star) == 2 && !CoalitionDefeated()) tstr2 = \"Система захвачена пиратами, часть станций построить невозможно!\";",
                        "else tstr2 = '';",
                        "",
                        "cost = GalaxyMoney(4,4) * 3;"
                    ]
                },
                {
                    "Type": "Top",
                    "Name": "",
                    "Pos.x": 290,
                    "Pos.y": -30,
                    "Parent": -1,
                    "#": 29,
                    "Code.Type": "Turn",
                    "Code": 
                    [
                        "j = 0;",
                        "for(k=0;StarShips(temp_star)>k;k=k+1) {",
                        "    if(ShipType(StarShips(temp_star,k)) == 'WB') j=j+1;",
                        "}",
                        "",
                        "if(StarRuins(temp_star) - j < 3 && StarCustomFaction(temp_star) == '') {",
                        "    if(!CoalitionDefeated() && !ShipOnSidePirateClan(Player()) && StarOwner(temp_star) == 0) {",
                        "        if(StarRuins(temp_star,'RC') == 0 && ShipMoney(Player()) >= cost) InjectAnswer('Build',\"Построить центр рейнджеров\",6);",
                        "        if(StarRuins(temp_star,'SB') == 0 && ShipMoney(Player()) >= cost) InjectAnswer('Build',\"Построить научную базу\",9);",
                        "        if(StarRuins(temp_star,'BK') == 0 && ShipMoney(Player()) >= cost) InjectAnswer('Build',\"Построить бизнес-центр\",10);",
                        "    }",
                        "    if(StarRuins(temp_star,'MC') == 0 && StarOwner(temp_star) != 1 && ShipMoney(Player()) >= cost) InjectAnswer('Build',\"Построить медицинский центр\",11);",
                        "    if(ShipOnSidePirateClan(Player())) {",
                        "        if(StarRuins(temp_star,'PB') != 1 && ShipMoney(Player()) >= cost && StarOwner(temp_star) != 1) InjectAnswer('Build',\"Построить пиратскую базу\",7);",
                        "        if(StarOwner(temp_star) == 2 && ShipMoney(Player()) >= cost) InjectAnswer('Build',\"Построить доминион\",12);",
                        "    }",
                        "    if(CurrentMods('Expansion\\ExpTC') /*&& CurrentMods('Expansion\\ExpPilotBridge')*/) {",
                        "        if(StarRuins(temp_star,'TC') == 0 && StarOwner(temp_star) != 1 && ShipMoney(Player()) >= cost) InjectAnswer('Build',\"Построить технологический центр\",13);",
                        "    }",
                        "}",
                        "",
                        "//Слава системам Мажино!",
                        "",
                        "if(StarOwner(temp_star) == 0 && ShipMoney(Player()) >= cost && j<6 && !ShipOnSidePirateClan(Player()) && StarCustomFaction(temp_star) == '') {",
                        "    InjectAnswer('Build',\"Построить военную базу\",8);",
                        "}"
                    ]
